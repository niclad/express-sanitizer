{
  "name": "sanitware",
  "version": "0.1.1",
  "description": "Express middleware to sanitize JSON responses",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest",
    "ts-node": "ts-node src/index.ts",
    "version": "npm run format && git add -A src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niclad/express-sanitizer.git"
  },
  "keywords": [
    "sanitizer",
    "sanitize",
    "Express",
    "middleware",
    "json",
    "response"
  ],
  "author": "Nick T",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/niclad/express-sanitizer/issues"
  },
  "homepage": "https://github.com/niclad/express-sanitizer#readme",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@jest/globals": "^29.5.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-jest": "^29.5.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist"
  ]
}
